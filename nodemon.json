{
  "restartable": "rs",
  "ignore": [
    ".git",
    ".svn",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "views/**/*.tpl",
    "views/*.tpl",
    "public/**/*.*",
    "routes/*.*"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json njk css tpl "
}